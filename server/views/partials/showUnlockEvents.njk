{% macro showUnlockEvents(events) %}
    <div class="govuk-body govuk-!-margin-0 govuk-!-font-size-14">
        {% for e in events %}
            {% if not loop.first %}<hr class="mid-grey-tint-20-hr" />{% endif %}
            <div class="govuk-!-font-weight-bold">{{ e.event }}</div>
            <div>{{ e.startTime }}{% if e.endTime %} - {{ e.endTime }}{% endif %}<div>
            <div>{{ e.location | title }}</div>
        {% endfor %}
    </div>
{% endmacro %}
