{% extends "layout.njk" %}

{% from "govuk/components//button/macro.njk" import govukButton %}
{% from "govuk/components/panel/macro.njk" import govukPanel %}

{% set pageTitle = applicationName + " - Planned events" %}
{% set pageId = "planned-events-page" %}

{% block content %}
    <div class="govuk-grid-row">
        <div class="govuk-grid-column-two-thirds">
            {{ govukPanel({
                titleText: "Planned events"
            }) }}

            <h2 class="govuk-heading-m">What parameters was I called with?</h2>
            <div class="govuk-body">
                <p>DatePresetOption {{ plannedDatePresetOption }}</p>
                <p>Date {{ plannedDate }}</p>
                <p>Slot {{ plannedSlot }}</p>
                <p>Locations {{ plannedLocations }}</p>
            </div>
        </div>
    </div>
{% endblock %}
